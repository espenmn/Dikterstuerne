<configure
    xmlns="http://namespaces.zope.org/zope"
    xmlns:five="http://namespaces.zope.org/five"
    xmlns:i18n="http://namespaces.zope.org/i18n"
    xmlns:genericsetup="http://namespaces.zope.org/genericsetup"
    xmlns:plone="http://namespaces.plone.org/plone"
    xmlns:browser="http://namespaces.zope.org/browser"
    i18n_domain="medialog.mobilethemeWessel">
    
  <five:registerPackage package="." initialize=".initialize" />
  
  <genericsetup:registerProfile
      name="default"
      title="medialog.mobilethemeWessel"
      directory="profiles/default"
      description="Installs the medialog.mobilethemeWessel package"
      provides="Products.GenericSetup.interfaces.EXTENSION"
      />

  <plone:static type="theme" directory="static" />

  <include package="z3c.jbot" file="meta.zcml" />
  
  <!--browser:jbot
    directory="static/overrides"
    layer="medialog.mobilethemeWessel.interfaces.IMobilethemeWesselLayer" /-->
      
  <browser:page
    name="cache.manifest3"
    for="plone.app.layout.navigation.interfaces.INavigationRoot"
    class=".views.Manifest"
    permission="zope2.Public"
  />
     
</configure>
